<h1>All Songs</h1>
<table>
<tr>
<th>Title</th>
<th>Genre</th>
<th>Artist Name</th>
<th>Release Year</th>
</tr>

<% @songs.each do |s| %>
    <tbody>
    <tr>
    <td><%= s.title %></td>
    <td<%= s.genre %></td>
    <td<%= s.artist_name %></td>
    <td<%= s.release_year %></td>
    </tr>
    </tbody>

    <% end %>
    </table>
